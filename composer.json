{
    "name": "hexlet/code",
    "bin": [
	      "bin/brain-games",
	      "bin/brain-even",
          "bin/brain-calc",
          "bin/brain-gcd",
          "bin/brain-progression",
          "bin/brain-prime"
    ],
    "description": "Mind Games is a set of five console games built on the principle of popular mobile applications for brain training. Each game asks questions that must be answered correctly. After three correct answers, the game is considered completed. Wrong answers end the game and prompt you to play it again.",
    "autoload": {
        "psr-4": {
            "Braingames\\": "src"
        },
        "files": [
            "src/Engine.php",
            "src/Games/EvenOrNot.php",
            "src/Games/BrainGcd.php",
            "src/Games/BrainProgression.php",
            "src/Games/BrainPrime.php",
            "src/Games/BrainCalculation.php"
        ]
    },
    "authors": [
        {
            "name": "Sergey Danilov",
            "email": "danilovserg1985s@gmail.com"
        }
    ],
    "license": "proprietary",
    "require": {
        "wp-cli/php-cli-tools": "^0.11.21"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7"
    }
}
